{
  "name": "mydiary",
  "version": "1.0.0",
  "description": "My Journaling app",
  "main": "index.js",
  "scripts": {
    "db:migrate": "sequelize db:migrate",
    "start": "node build/index.js",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "build": "rm -rf build && mkdir build && babel api --out-dir build",
    "dev": "nodemon --exec babel-node api/index.js",
    "test": "cross-env NODE_ENV=test nyc mocha --require @babel/register ./test/*.test.js --exit || true"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-react": "^7.17.0",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "pg": "^7.17.0"
  },
  "dependencies": {
    "sequelize": "^5.21.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "sequelize-cli": "^5.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dieudonneAwa/myDiary.git"
  },
