{
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
          "modules": true,
          "jsx": true
      }
    },
    "plugins": [
        "react", "import"
    ],
    "rules": {
      "linebreak-style": 0,
      "class-methods-use-this": 0,
      "import/prefer-default-export": 0
    }
}